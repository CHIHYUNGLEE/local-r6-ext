	<query name="getWorkChatByWorkId">
	<![CDATA[
		FROM WorkChat AS u WHERE u.workId = :workId
	]]>
	</query>
	
	<query name="getLastVersionFileByItemId">
	<![CDATA[
		FROM Work$WorkAttachment AS u WHERE u.lastVersion = true AND u.item.id = :itemId
	]]>
	</query>
	
	<query name="getNotLastVersionFileByItemId">
	<![CDATA[
		FROM Work$WorkAttachment AS u WHERE u.lastVersion = false AND u.item.id = :itemId
	]]>
	</query>
	
	<query name="getNotLastVersionReportByItemId">
	<![CDATA[
		FROM Work$ReportFile AS u WHERE u.lastVersion = false AND u.item.id = :itemId
	]]>
	</query>
	
	<query name="getLastVersionReportByItemId">
	<![CDATA[
		FROM Work$ReportFile AS u WHERE u.lastVersion = true AND u.item.id = :itemId
	]]>
	</query>
	
	<query name="getProcessByGid">
	<![CDATA[
		FROM WorkProcess AS u WHERE u.gid = :gid order by case when eventDate is not null then 1 else 0 end desc, eventDate desc
	]]>
	</query>
	
	<query name="getProcessByItemId">
	<![CDATA[
		FROM WorkProcess AS u WHERE u.item.id = :itemid order by u.id desc, u.reqDate desc
	]]>
	</query>		
